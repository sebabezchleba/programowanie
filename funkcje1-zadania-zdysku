#NOTE: zad 16

def sprawdz_dol(liczba):
    if liczba>10:
        wynik="Podana liczba jest większa od 10."
    else:
        wynik="Podana liczba jest mniejsza bądź równa 10."
    return wynik

def sprawdz_gore(liczba):
    if liczba<100:
        wynik="Podana liczba jest mniejsza od 100."
    else:
        wynik="Podana liczba jest większa bądź równa 100."
    return wynik

def pierwiastkuj(liczba):
    wynik=liczba**0.5
    return wynik

def dodaj_dwa(liczba):
    liczba=pierwiastkuj(liczba)
    wynik=liczba+2
    return wynik

def do_potegi(liczba):
    liczba=dodaj_dwa(liczba)
    wynik=liczba**2
    return wynik

def wyswietl(liczba):
    wynik=do_potegi(liczba)
    return "Początkowa liczba to "+str(liczba)+". Końcowa liczba to "+str(wynik)

print("Podaj liczbę.")
liczba=int(input())
dol=sprawdz_dol(liczba)
gora=sprawdz_gore(liczba)
wynik=wyswietl(liczba)
print(dol)
print(gora)
print(wynik)
